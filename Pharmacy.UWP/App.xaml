<Application
    x:Class="Pharmacy.UWP.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Pharmacy.UWP">

    <Application.Resources>
        <ResourceDictionary>
            <local:ToBitmapConverter x:Key="ToBitmapConverter"/>
            <local:ToStringFromDateTimeConverter x:Key="ToStringFromDateTimeConverter"/>
            <local:ToNameConverter x:Key="ToNameConverter"/>
            <local:ToImageConverter x:Key="ToImageConverter"/>
            <local:ToPriceConverter x:Key="ToPriceConverter"/>
            <local:FromBasketsID_Converter x:Key="FromBasketsID_Converter"/>
            <local:ToDateAndTimeConverter x:Key="ToDateAndTimeConverter"/>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
